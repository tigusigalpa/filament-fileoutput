{
    "name": "tigusigalpa/filament-fileoutput",
    "description": "A Laravel Filament plugin for displaying uploaded files (including private files)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Sazonov"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "filament/forms": "^3.0|^4.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Tigusigalpa\\FileOutput\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tigusigalpa\\FileOutput\\FileOutputServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
